library(dplyr)
library(wooldridge)
data("wage1")
# 男女別の賃金の平均値・標準偏差を計算する。
wage1_summary <- wage1 %>%
group_by(female) %>%
summarise(
mean_wage = mean(wage, na.rm = TRUE),
sd_wage = sd(wage, na.rm = TRUE)
)
print(wage1_summary)
